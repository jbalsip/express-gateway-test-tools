{
  "name": "express-gateway-test-tools",
  "version": "0.2.4",
  "description": "Tools for assisting with testing and validating custom plugin code",
  "keywords": [
    "express-gateway",
    "testing"
  ],
  "main": "index.js",
  "scripts": {
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix",
    "test": "cross-env EG_DISABLE_CONFIG_WATCH=true jest",
    "test:ci": "npm test -- --ci --reporters=default --reporters=jest-junit"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/matcha440/express-gateway-test-tools.git"
  },
  "author": "Jason Alsip",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/matcha440/express-gateway-test-tools/issues"
  },
  "homepage": "https://github.com/matcha440/express-gateway-test-tools#readme",
  "devDependencies": {
    "cross-env": "^5.1.3",
    "eslint": "^4.17.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jest": "^22.0.0",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-node": "^6.0.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "express-gateway": "^1.13.0",
    "jest": "^23.6.0",
    "jest-junit": "^5.2.0",
    "lint-staged": "^6.1.0",
    "supertest": "^3.0.0"
  }
}
